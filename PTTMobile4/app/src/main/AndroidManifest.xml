<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.pttmobile4">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".admin.EditUserActivity"></activity>
        <activity android:name=".admin.CreateUserActivity" />
        <activity android:name=".admin.AdminActivity" />

        <activity android:name=".user.CreateProjectActivity" />
        <activity android:name=".user.ProjectActivity" />
        <activity android:name=".user.UserActivity" />
        <activity android:name=".user.UserLoginActivity" />

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>